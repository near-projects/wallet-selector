;\n  }\n}\n\nif (typeof window !== \"undefined\") {\n  window.__ledgerLogsListen = listen;\n}\n"},"lineCount":null}},"error":null,"hash":"8992a7251db25cb364f8959f39cc90c3","cacheData":{"env":{}}}